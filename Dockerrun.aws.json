{
 "AWSEBDockerrunVersion": 2,
 "containerDefinitions": [
  {
   "name": "client",
   "image": "lienejansone/multi-client",
   "hostname": "client",
   "essential": false
  },
  {
   "name": "server",
   "image": "lienejansone/multi-server",
   "hostname": "api",
   "essential": false
  },
  {
   "name": "worker",
   "image": "lienejansone/multi-worker",
   "hostname": "worker",
   "essential": false
  },
  {
   "name": "nginx",
   "image": "lienejansone/multi-nginx",
   "essential": true,
   "portMappings": [
     {
      "hostPort": 80,
      "containerPort": 80
     }
   ],
   "links": ["client", "server"]
  }
 ]
}

